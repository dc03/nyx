fn go(x: int, acc: int) -> int {
    if x > 1 {
        return go(x - 1, acc * x)
    } else {
        return acc
    }
}

fn factorial(x: int) -> int {
    return go(x, 1)
}

fn main() -> int {
    var x = int(readline("Enter the literal to take the factorial of: "))
    print("The factorial of " + string(x) + " is: " + string(factorial(x)))
    return 0
}

main()
